<div class="wrapper">
    <header>
        {{#if loggedIn}}
            <div class="user-info">
                <p class="user-name"><b>User Name: </b>{{user.name.first}} {{user.name.last}}</p>
                <p class="user-email"><b>User Email: </b>{{user.email}}</p>
                <p class="user-role"><b>User Role: </b>{{user.role}}</p>
            </div>
        {{else}}
            <button class="login">Log In</button>
        {{/if}}
    </header>
</div>
